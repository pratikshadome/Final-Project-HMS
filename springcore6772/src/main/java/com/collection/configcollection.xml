              <value>listening songs</value>  
                <value>reading books</value>   
                  <value>travelling</value>   
                    <value>nighout on seaside</value>          
         
         </list>      
      
      </property>
      
      <property name="contact">
      <set>
              <value>98574322344</value>
              <value>98574325678</value>
              <value>24557322344</value>
              <value>98574343567</value>
      </set>
      </property>
      
      <property name = "mobile">
      <map>
              <entry key="nokia" value = "50000">  </entry>
              <entry key="samsung" value = "80000"></entry>
              <entry key="apple" value="90000"></entry>
              <entry key="redmi" value="60000"></entry>
      </map>
      
